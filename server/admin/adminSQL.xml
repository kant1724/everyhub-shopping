<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="adminSQL">
    <select id="selectProductList">
        SELECT
          ITEM_NO,
          ITEM_NM_1,
          ITEM_NM_2,
          PRICE,
          ITEM_KCD,
          ORIGIN_CD,
          IMAGE_PATH,
          ITEM_DESC
        FROM
          ITEMS
  </select>
</mapper>